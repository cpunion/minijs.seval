{
	"name": "minijs-seval",
	"version": "1.0.1",
	"description": "MiniJS compiler written in S-expressions, self-hosted on seval",
	"type": "module",
	"main": "minijs.seval",
	"types": "index.d.ts",
	"exports": {
		".": {
			"types": "./index.d.ts",
			"default": "./minijs.seval"
		},
		"./source": {
			"types": "./index.d.ts",
			"default": "./minijs.seval"
		},
		"./minijs.seval": {
			"types": "./index.d.ts",
			"default": "./minijs.seval"
		}
	},
	"files": ["minijs.seval", "index.d.ts"],
	"scripts": {
		"test": "bun test",
		"test:watch": "bun test --watch",
		"lint": "biome check .",
		"lint:fix": "biome check --write .",
		"format": "biome format --write .",
		"format:check": "biome format --write . && git diff --exit-code",
		"coverage": "bun test --coverage --coverage-reporter=lcov --coverage-reporter=text"
	},
	"keywords": ["minijs", "seval", "s-expression", "self-hosted", "compiler"],
	"author": "cpunion",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/cpunion/minijs.seval.git"
	},
	"dependencies": {
		"seval.js": "^1.2.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/bun": "latest"
	}
}
